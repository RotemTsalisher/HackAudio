load 'Ch6SignalParams';
f1 = 1;
x1 = sin(2*pi*f1.*t);

Apeak = max(abs(x1));
App = max(x1)-min(x1);
Arms = ((1/N)*sum(x1.^2))^(1/2);
CF = Apeak/Arms;

plot(t,x1,'black'); yline(Arms, 'black--');
ppArrowXcord = t(x1(x1==max(x1))); ppArrowYcord = max(x1); annotation('textarrow',ppArrowXcord,ppArrowYcord);